{
  "name": "snowflake-sink-connector",
  "config": {
    "connector.class": "com.snowflake.kafka.connector.SnowflakeSinkConnector",
    "tasks.max": "1",
    "snowflake.ingestion.method": "SNOWPIPE_STREAMING",
    "snowflake.enable.schematization": true,
    "topics": "mysqldb.inventory.users,postgresdb.inventory.product",
    "snowflake.topic2table.map": "mysqldb.inventory.users:cdc_mysql_inventory_users_test2,postgresdb.inventory.product:cdc_postgresdb_inventory_product_test2",
    "snowflake.url.name": "",
    "snowflake.user.name": "",
    "snowflake.role.name": "",
    "snowflake.private.key": "",
    "snowflake.private.key.passphrase": "",
    "snowflake.database.name": "",
    "snowflake.warehouse.name": "",
    "snowflake.schema.name": "PUBLIC",
    "behavior.on.null.values": "IGNORE"
  }
}